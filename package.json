    "gray-matter": "^4.0.3",
    "i18nexus-cli": "^3.1.0",
    "marked": "^4.0.18",
    "next": "^13.5.4",
    "next-i18next": "^12.0.1",
    "next-mdx-remote": "^4.1.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.4.0",
