    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-sznm": "^0.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
